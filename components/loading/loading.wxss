.k-line {
	display:inline-block;
	width:10px;
	height:15px;
	border-radius:15px;
  background-color:#52b4ff;
  margin-right: 6rpx;
}
.k-line1 {
	animation:k-loadingA 1.5s .1s infinite;
	background-color:#118fff
}
.k-line2 {
	animation:k-loadingA 1.5s .2s infinite;
	background-color:#0088ff
}
.k-line3 {
	animation:k-loadingA 1.5s .4s infinite;
	background-color:#46a8ff
}
.k-line4 {
	animation:k-loadingA 1.5s .8s infinite;
	background-color:#0083f5
}
.k-line5 {
	animation:k-loadingA 1.5s 1.2s infinite;
	background-color:#0062b8
}
@keyframes k-loadingA {
	0% {height:10px}
  50% {height:25px}
  100% {height:10px}
}
@keyframes k-loadingD {
  0% {transform:rotate(0deg)}
  50% {transform:rotate(180deg)}
  100% {transform:rotate(360deg)}
}
 .sk-three-bounce{
  position:relative;
  width: 200rpx;
  text-align: center;
  margin: 20rpx auto;
}